import java.awt.*;
import javax.swing.*;

public class Frame {

	public static void main(String[] args){
		new Frame();
	}
	
	public Frame(){
		JFrame gui = new JFrame();
		gui.setTitle("Test Paint");
		gui.setSize(400,400);
		gui.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		
		Something panel = new Something();
		
		Container pane = gui.getContentPane();
		
		pane.setLayout(new GridLayout(1,1);
		pane.add(panel);
		gui.setVisible(true);
	}
	
}
