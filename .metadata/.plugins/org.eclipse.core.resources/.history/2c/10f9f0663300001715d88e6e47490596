public class Spreadsheet implements Grid
{
	private Cell[][] array;
	
	public Spreadsheet(){
		array = new Cell [20][12];
		for(int i = 0; i < array.length; i++){
			for( int j = 0; j < array[i].length; j++){
				array[i][j] = new EmptyCell();
			}
		}
	}
	
	@Override
	public String processCommand(String command)
	{
