
public class TestCode {

	public static void main (String[] args){
		System.out.println(changePi("xpi"));
		System.out.println(changePi("pix"));
	}
	public static String changePi(String str) {
		if(str.length() <= 2){
			if(str.equals("pi")){
				return "3.14";
			}else{
				return str;
			}
		//}else if(str.indexOf("pi") >= 0){
				
		}else{
			return str;
		}
	}

}
